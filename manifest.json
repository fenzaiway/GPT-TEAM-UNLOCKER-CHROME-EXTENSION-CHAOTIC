{
    "manifest_version": 3,
    "name": "GPT Team Unlocker",
    "version": "2.0.0",
    "description": "GPT Team Unlocker helps temporarily unlock your regular ChatGPT account into a ChatGPT Team account with full advanced features such as accessing GPT-4, creating images with Dall E and more...",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "cookies"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "crypto-js.min.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "128": "logo_extension.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}
